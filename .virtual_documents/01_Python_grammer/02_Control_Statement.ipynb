money = 0
coffee = 10
remain = 0

while True :
    print("투입할 금액을 입력해 주세요.")
    money = int(input())
    if money >= 300 :
        remain = money - 300
        print("돈을 받았으니 커피를 줍니다.")
        if money > 300:
            print(f"거스름돈이 반환됩니다\n거스름돈: {remain}")
        coffee = coffee - 1        
        print(f"남은 커피의 양은 {coffee} 입니다.")
        if coffee == 0:
            print("커피가 다 떨어져서 판매를 중지합니다.")
            break
    else:
        print(f"투입한 금액이 부족합니다. 투입한 금액:{money}\n투입한 돈이 반환됩니다.")






