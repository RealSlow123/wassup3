def add_many(*args):
    result = 0
    for i in args:
        result += i
    return result

add_many(1,2,3,4,5)


def calculator(Operator, *args):
    result = 0
    if Operator == "*":
        result = 1
        
    for i in args:
        if Operator == "+":
            result += i
        elif Operator == "*":            
            result *= i
        else:
            result = 0        
    return result


calculator("*", 1,2,3,4,5)





